{
  "name": "marcusmonteirodesouza",
  "version": "1.0.0",
  "description": "Marcus Vinicius Monteiro de Souza's Personal Website",
  "main": "index.js",
  "scripts": {
    "get-about": "ts-node --files src/scripts/get-about",
    "check-types": "tsc -p . --noEmit",
    "prebuild": "yarn get-about && yarn check-types",
    "build": "gatsby build",
    "prestart": "yarn get-about",
    "start": "gatsby develop",
    "clean": "gatsby clean && rm -rf node_modules",
    "serve": "yarn build && gatsby serve -p 9000",
    "format": "prettier --write .",
    "lint": "eslint --fix --ext .js,.ts,.tsx .",
    "setup": "node ./bin/setup.js",
    "test": "percy exec -t 350 -h cdn-images-1.medium.com -- cypress run",
    "test:open": "percy exec -t 350 -h cdn-images-1.medium.com -- cypress open",
    "test:ci": "DETERMINISTIC=true start-server-and-test 8000",
    "update-media": "cypress run -C ./cypress-media.json",
    "update-media:ci": "DETERMINISTIC=true start-server-and-test 8000 update-media"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcusmonteirodesouza/marcusmonteirodesouza.git"
  },
  "keywords": [
    "gatsby",
    "contentful",
    "PWA",
    "portfolio"
  ],
  "author": "Marcus Vinicius Monteiro de Souza",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marcusmonteirodesouza/marcusmonteirodesouza/issues"
  },
  "homepage": "https://github.com/marcusmonteirodesouza/marcusmonteirodesouza#readme",
  "dependencies": {
    "@babel/preset-env": "^7.12.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@rebass/preset": "^4.0.5",
    "@tippy.js/react": "^3.1.1",
    "contentful": "^7.14.3",
    "gatsby": "^2.24.86",
    "gatsby-plugin-google-analytics": "^2.3.19",
    "gatsby-plugin-manifest": "^2.4.35",
    "gatsby-plugin-offline": "^3.2.37",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-styled-components": "^3.3.10",
    "gatsby-plugin-typescript": "^2.4.22",
    "gatsby-source-contentful": "^2.3.55",
    "gatsby-source-medium": "^2.3.14",
    "gatsby-transformer-remark": "^2.8.46",
    "prop-types": "^15.6.2",
    "react": "^16.14.0",
    "react-awesome-reveal": "^3.3.1",
    "react-dom": "^16.14.0",
    "react-headroom": "^3.0.0",
    "react-helmet": "^6.1.0",
    "react-markdown": "^4.3.1",
    "react-scroll-section": "^3.0.2",
    "react-text-loop": "^2.0.1",
    "rebass": "^4.0.7",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "@percy/cypress": "^2.1.1",
    "@types/react-headroom": "^2.2.1",
    "@types/react-helmet": "^6.1.0",
    "@types/rebass": "^4.0.7",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "axe-core": "^4.0.2",
    "babel-eslint": "^10.1.0",
    "chalk": "^4.1.0",
    "contentful-import": "^7.9.14",
    "cypress": "^4.12.1",
    "cypress-axe": "^0.9.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.12.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.5",
    "inquirer": "^7.3.3",
    "polished": "^4.0.3",
    "prettier": "^2.1.2",
    "start-server-and-test": "^1.11.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
}
